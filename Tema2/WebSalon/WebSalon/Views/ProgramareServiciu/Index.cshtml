@model IEnumerable<WebSalon.Models.ProgramareServiciu>

@{
    ViewData["Title"] = "Index";
}

<h1>Asociere serviciu la programare</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Serviciu)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Programare)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Serviciu.numeServiciu)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Programare.dataOra)
            </td>
        </tr>
}
    </tbody>
</table>
